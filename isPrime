import java.util.*;
class Main{
	
	public static boolean isPrime(int a){
		if(a<=1){
			return false;
		}
		for(int i=2 ; i<=a-1 ; i++){
			if((a%i)==0)
				return false;	
		}
		return true;
		
	}

	
	
	
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		boolean ans = isPrime(N);
		System.out.println(ans);
		
	}
}
